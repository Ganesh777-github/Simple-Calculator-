#include<iostream>
using namespace std;
int main(){
    int n1,n2;
    char c;
    int r=0;
    cout<<"enter your numbers:";
    cin>>n1>>n2;
    cout<<"Enter operation(+,-,*,/) :";
    cin>>c;
    if(c=='+'){
        r=n1+n2;
    }
    else if(c=='-'){
        r=n1-n2;
    }
    else if(c=='*'){
        r=n1*n2;
    }
    else if(c=='/'){
        r=n1/n2;
    }
    else{
        cout<<"Invalid input!!";
    }
    cout<<r<<endl;
}
